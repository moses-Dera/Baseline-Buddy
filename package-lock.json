{
  "name": "baseline-buddy",
  "version": "1.0.0",
  "lockfileVersion": 3,
  "requires": true,
  "activationEvents": [
    "onCommand:baseline-buddy.helloWorld",
    "onCommand:baseline-buddy.checkFeature"
  ],
  "description": "A VS Code extension that checks if web features are part of Baseline, warns about risky features, and suggests safer alternatives.",
  "displayName": "Baseline Buddy",
  "license": "MIT",
  "main": "./extension.js",
  "packages": {
    "": {
      "name": "baseline-buddy",
      "version": "1.0.0",
      "dependencies": {
        "web-features": "^3.2.0"
      },
      "engines": {
        "vscode": "^1.85.0"
      }
    },
    "node_modules/web-features": {
      "version": "3.2.0",
      "resolved": "https://registry.npmjs.org/web-features/-/web-features-3.2.0.tgz",
      "integrity": "sha512-sq3jriY7/+ejkX5ECYBc+G6a2KWdmcGSNqThFfi+fl46JloyxWLFT/3WOLMOvtbLDpZFY2CYt6/VEkaJKSXeHA==",
      "license": "Apache-2.0"
    }
  },
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "contributes": {
    "commands": [
      {
        "command": "baseline-buddy.helloWorld",
        "title": "Baseline Buddy: Hello World"
      },
      {
        "command": "baseline-buddy.checkFeature",
        "title": "Baseline Buddy: Check Web Feature"
      }
    ]
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/baseline-buddy"
  }
}
